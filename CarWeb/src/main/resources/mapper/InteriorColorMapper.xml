<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nd.car.web.dao.InteriorDao">
    <!--    在这里写数据库查询语句，id即为接口方法的名字-->
    <select id="findAllInteriorColor" resultType="com.nd.car.web.entity.InteriorColor">
        SELECT * FROM interior ORDER BY count DESC LIMIT 20
    </select>
</mapper>